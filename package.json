{
  "name": "cyano-wallet-desktop",
  "description": "An Ontology blockchain wallet",
  "version": "1.0.0",
  "displayName": "Cyano wallet Desktop",
  "author": "Matus Zamborsky <zamborsky@gmail.com>",
  "license": "LGPL-3.0-or-later",
  "main": "out/main.js",
  "build": {
    "appId": "zamborsky.cyano-wallet",
    "files": [
      "out",
      "assets",
      "build"
    ],
    "mac": {
      "category": "public.app-category.finance"
    }
  },
  "scripts": {
    "copy-css": "cpy ./assets/global.css out && cpy ./node_modules/semantic-ui-css/semantic.min.css out",
    "copy-html": "cpy ./src/popup.html out ",
    "copy-semantic": "cpy '**/*' ../../../out/themes --cwd=node_modules/semantic-ui-css/themes --parents",
    "copy": "npm run copy-css && npm run copy-html && npm run copy-semantic",
    "build-js": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "electron ./out/main.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "npm run copy && npm run build-js"
  },
  "devDependencies": {
    "@types/electron-json-storage": "^4.0.0",
    "@types/file-saver": "^2.0.0",
    "@types/lodash": "^4.14.118",
    "@types/long": "^4.0.0",
    "@types/promise-timeout": "^1.3.0",
    "@types/qrcode.react": "^0.8.1",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-file-reader-input": "^1.1.3",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.4.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.4",
    "cpy-cli": "^2.0.0",
    "electron": "^3.0.4",
    "electron-builder": "^20.36.2",
    "electron-rebuild": "^1.8.2",
    "remove": "^0.1.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@ont-community/ontology-ts-sdk-ledger": "^1.0.8",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/node": "^10.0.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "date-fns": "^1.29.0",
    "electron-json-storage": "^4.1.4",
    "express": "^4.16.4",
    "fast-deep-equal": "^2.0.1",
    "file-loader": "0.11.2",
    "file-saver": "^1.3.8",
    "final-form": "^4.8.1",
    "lodash": "^4.17.10",
    "long": "^4.0.0",
    "ontology-dapi-desktop": "^1.0.0",
    "ontology-ts-sdk": "^1.0.2",
    "ontology-ts-test": "^0.2.33",
    "promise-timeout": "^1.3.0",
    "prop-types": "^15.6.1",
    "qrcode.react": "^0.8.0",
    "react": "^16.4.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.0",
    "react-file-reader-input": "^1.1.4",
    "react-final-form": "^3.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-electron-store": "^0.6.3",
    "resolve": "1.6.0",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.81.1",
    "uuid": "^3.2.1",
    "websocket-as-promised": "^0.8.0",
    "whatwg-fetch": "2.0.3"
  }
}
